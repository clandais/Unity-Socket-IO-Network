  "main": "index.js",
  "scripts": {
    "browser-livereload": "browser-sync start --server docs~/ --port 8080 --files='docs~/*'",
    "watch": "start /min npm-watch build & start /min npm run browser-livereload",
    "build": "docfx build .docfx_project/docfx.json"
  },
  "watch": {
    "build": {
      "patterns": [ "./*" ],
      "extensions": "md,yml,json",
      "quiet": false,
      "ignore": "_site/*",
      "delay": 500
    }
  },
  "devDependencies": {
    "npm-watch": "^0.11.0",
    "browser-sync": "^2.27.11"
  }
}
